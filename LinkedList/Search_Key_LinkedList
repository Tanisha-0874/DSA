Question Link:https://www.geeksforgeeks.org/problems/search-in-linked-list-1664434326/0


/* Link list node */
/*
struct Node
{
    int data;
    Node* next;
    Node(int x) {  data = x;  next = NULL; }
}; */
    
class Solution {
  public:
    // Function to count nodes of a linked list.
    bool searchKey(int n, Node* head, int key) {
       Node* p=head;
       while(p!=NULL){
           if(p->data==key){
               return true;
           }
           p=p->next;
       }
        return false;
    }
};
